<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="10dp"
                android:height="10dp"/>
            <solid android:color="#50ff5e34"/>
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <size
                android:width="4dp"
                android:height="4dp"/>
            <solid android:color="#ff5e34"/>
        </shape>
    </item>
</layer-list>